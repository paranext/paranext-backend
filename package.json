    "webpack-merge": "^5.8.0"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**",
      "src/node/**",
      "src/shared/**"
    ],
    "logLevel": "quiet"
  },
  "nodemonConfig": {
    "watch": [
      "src/client/**",
      "src/extension-host/**",
      "src/node/**",
      "src/shared/**"
    ]
  }
}
