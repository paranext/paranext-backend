    <PackageReference Include="SIL.WritingSystems" Version="12.0.0.0" />
    <PackageReference Include="System.Net.WebSockets" Version="4.3.0" />
    <PackageReference Include="icu.net" Version="2.9.0" />
  </ItemGroup>

</Project>
